html{-webkit-text-size-adjust:none;font-size:10px;-webkit-font-smoothing:antialiased;}
body{background-color:#ffffff;color:#444444;font-family: 'Roboto',Arial, sans-serif;font-size:1.6rem;line-height:2.8rem;}
h1, .h1{font-family: PoynterOSDisp-Roman;font-size: 5.4rem;font-weight: normal;font-stretch: normal;font-style: normal;line-height: 6.8rem;letter-spacing: normal;color: #001e49;}
h2, .h2{font-family: PoynterOSDisp-Roman;font-size: 4.2rem;font-weight: normal;font-stretch: normal;font-style: normal;line-height: 5.6rem;letter-spacing: normal;color: #001e49;}
h3, .h3{font-family: PoynterOSDisp-Roman;font-size: 3.0rem;font-weight: normal;font-stretch: normal;font-style: normal;line-height: 4.0rem;letter-spacing: normal;color: #001e49;}
h4, .h4{font-family: PoynterOSDisp-Roman;font-size: 2.5rem;font-weight: normal;font-stretch: normal;font-style: normal;line-height: 3.6rem;letter-spacing: normal;color: #001e49;}
h5, .h5{font-family: 'Roboto',Arial, sans-serif;font-size: 2.0rem;font-weight: normal;font-stretch: normal;font-style: normal;line-height: 3.6rem;letter-spacing: normal;color: #444444;}
p, .body{font-family: 'Roboto',Arial, sans-serif;font-size: 1.6rem;font-weight: normal;font-stretch: normal;font-style: normal;line-height: 2.8rem;letter-spacing: normal;color: #444444;}
.intro,.intro p{font-family: 'Roboto',Arial, sans-serif;font-size: 1.8rem;font-weight: normal;font-stretch: normal;font-style: normal;line-height: 3.2rem;letter-spacing: normal;color: #444444;}
.tag{font-family: 'Roboto',Arial, sans-serif;font-size: 1.4rem;font-weight: normal;font-stretch: normal;font-style: normal;line-height: 1.8rem;letter-spacing: normal;color: #444444;}
.rad-notes,.footnote,.footnote p{font-size: 1.4rem; line-height: 2.0rem;}
a{color:#083b92;} 
a:hover{color:#083b92;}
a:focus{text-decoration:none;outline: 1px dashed;}
input:focus{outline: 1px dashed;}

ul{padding: 0;margin: 0}
ul li{list-style: none}
.invisible {position: absolute;left: -9999999px;top: auto;width: 1px;height: 1px;overflow: hidden;visibility: visible !important;}
.container-fluid {max-width: 1320px;}
/*body.noScroll {overflow-x: hidden;}*/
.visuallyhidden {
    clip: rect(0 0 0 0);
    height: 1px;
    width: 1px;
    margin: -1px;
    padding: 0;
        padding-right: 0px;
    overflow: hidden;
    position: absolute;
}




@media (max-width: 991.98px) {
